{
  "dataRoles": [
    {
      "displayName": "Stream ID",
      "name": "stream",
      "kind": "Grouping"
    },
    {
      "displayName": "Object ID",
      "name": "object",
      "kind": "GroupingOrMeasure"
    },
    {
      "displayName": "Object Data",
      "name": "objectData",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "select": [
            {
              "for": {
                "in": "stream"
              }
            },
            {
              "for": {
                "in": "object"
              }
            }
          ]
        },
        "values": {
          "for": { "in": "objectData" }
        }
      }
    }
  ],
  "supportsHighlight": true,
  "supportsMultiVisualSelection": true,
  "suppressDefaultTitle": true,
  "tooltips": {
    "supportEnhancedTooltips": true
  },
  "drilldown": {
    "roles": ["stream", "object"]
  },
  "privileges": [
    {
      "name": "WebAccess",
      "essential": true,
      "parameters": [
        "https://speckle.xyz",
        "https://*.speckle.xyz",
        "https://*.speckle.dev"
      ]
    },
    {
      "name": "ExportContent",
      "essential": false
    }
  ]
}
