{
  "dataRoles": [
    {
      "displayName": "Stream ID",
      "name": "stream",
      "kind": "Grouping"
    },
    {
      "displayName": "Object ID",
      "name": "object",
      "kind": "GroupingOrMeasure"
    },
    {
      "displayName": "Object Data",
      "name": "objectData",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "select": [
            {
              "for": {
                "in": "stream"
              }
            },
            {
              "for": {
                "in": "object"
              }
            }
          ]
        },
        "values": {
          "for": { "in": "objectData" }
        }
      }
    }
  ],
  "supportsHighlight": true,
  "supportsMultiVisualSelection": true,
  "suppressDefaultTitle": true,
  "tooltips": {
    "supportEnhancedTooltips": true
  },
  "drilldown": {
    "roles": ["stream", "object"]
  },
  "objects": {
    "camera": {
      "displayName": "My Object Name",
      "properties": {
        "orthoMode": {
          "displayName": "Ortho camera",
          "type": { "bool": true }
        },
        "defaultView": {
          "displayName": "Default view",
          "type": {
            "enumeration": [
              {
                "displayName": "Top",
                "displayNameKey": "top",
                "value": "top"
              },
              {
                "displayName": "Front",
                "displayNameKey": "front",
                "value": "front"
              },
              {
                "displayName": "Left",
                "displayNameKey": "left",
                "value": "left"
              },
              {
                "displayName": "Back",
                "displayNameKey": "back",
                "value": "back"
              },
              {
                "displayName": "Right",
                "displayNameKey": "right",
                "value": "right"
              }
            ]
          }
        }
      }
    },
    "colors": {
      "displayName": "Colors",
      "properties": {}
    }
  },
  "privileges": [
    {
      "name": "WebAccess",
      "essential": true,
      "parameters": [
        "https://speckle.xyz",
        "https://*.speckle.xyz",
        "https://*.speckle.dev"
      ]
    },
    {
      "name": "ExportContent",
      "essential": false
    }
  ]
}
